.i-sticky-item-header {
    position: relative;
    background: #eee;
    font-size: 14px;
    width: 100%;
    height: 32px;
    line-height: 32px;
    z-index: -20;
}

.i-sticky-item-content {
    font-size: 14px;
    z-index: 1;
}

.i-sticky-title {
    width: 100%;
    padding: 0 15px;
    box-sizing: border-box;
    background: #eee;
}

.i-sticky-fixed .i-sticky-title {
    position: fixed;
    top: 0;
    z-index: -20;
}